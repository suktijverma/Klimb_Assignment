<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>ExcelToDb</title>
    <link rel="stylesheet" href="view-source:https://bootswatch.com/cosmo/">
    <link rel="stylesheet" href="https://bootswatch.com/4/cosmo/bootstrap.css">
</head>
<body>
<div class="fixed-top" style="background-color:#9675755e">
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
                <a class="navbar-brand" href="#">Upload Excel File</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                </button>
              </nav>
</div>

<div class="row" style="margin-top:125px;margin-left:40%; margin-right:10%; width: fit-content;">
    <div class="lg-6">
            <div class="card border-danger mb-3" style="max-width: 20rem;">
                    <div class="card-header">Upload Excel File</div>
                        <div class="card-body" style="background-color: rgb(187, 134, 134) ">
                          <form action="/" method="POST" enctype="multipart/form-data">
                         <div class="form-group">
                           <label for="excel">BrowseFile</label> 
                           <input type="file" class="form-control" name="excel" style="background-color: #d9c2b0;" required><br><br>
                           <input type="submit" value="submit" onclick="" style="background-color:rgb(163, 180, 129)">
                         </div>
                          </form>
                        </div>
                    </div>
                </div>
</div><br><br>

 <%if(result.length>0){%>
        <table class="table table-hover" border="1">
          <thead>
          <tr>
            <th>Serial No.</th>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile No.</th>
            <th>Date of birth</th>
            <th>Experience</th>
            <th>Resume title</th>
            <th>Location</th>
            <th>Address</th>
            <th>Employer</th>
            <th>Designation</th>
          </tr>
        </thead>
        <tbody>
          <%for(var x=0;x < result.length;x++){%>
            <tr>
              <td><%= (x+1)+'.' %></td>
              <td><%= result[x].name %></td>
              <!-- <td><%= result[x].class %></td>
              <td><%= result[x].subject %></td> -->
              <td><%= result[x].email %></td>
              <td><%= result[x].mobile %></td>
              <td><%= result[x].dob %></td>
              <td><%= result[x].experience %></td>
              <td><%= result[x].resume %></td>
              <td><%= result[x].location %></td>
              <td><%= result[x].address %></td>
              <td><%= result[x].employer %></td>
              <td><%= result[x].designation %></td>
            </tr>
            <%}%>
        </tbody>
        </table>
  <%}%>

<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>   
</body>
</html>


